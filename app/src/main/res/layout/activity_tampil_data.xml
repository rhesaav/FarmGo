        <TextView

            android:layout_weight="1"
            android:background="#cac9c9"
            android:gravity="center"
            android:padding="20dip"
            android:textColor="#000000" />

    </TableRow>

    <!--table 3-->

    <TableRow
        android:id="@+id/tableRow10"
