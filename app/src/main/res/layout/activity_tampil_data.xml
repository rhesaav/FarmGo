            android:layout_weight="1"
            android:background="#d3d3d3"
            android:gravity="center"
            android:padding="20dip"
            android:text=""
            android:textColor="#000000" />

        <TextView

            android:layout_weight="1"
            android:background="#cac9c9"
            android:gravity="center"
